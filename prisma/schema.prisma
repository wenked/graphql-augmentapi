generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model augments {
  id        Int      @id @default(autoincrement())
  name      String?
  tier      String?
  pickrate  String?
  winrate   String?
  top4      String?
  stage14   Float?
  stage33   Float?
  stage46   Float?
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
  placement Float?
  img       String?
  api_name  String?  @db.VarChar(255)
}

model matches {
  id        Int      @id @default(autoincrement())
  matchid   String?
  elo       String?
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}

model augments_match_data {
  id           Int      @id @default(autoincrement())
  matchid      String?
  elo          String?
  game_version String?
  placement    String?
  augment      String?
  api_name     String?
  tier         String?
  round        String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @default(now())
}

model historic_stats {
  id         Int      @id @default(autoincrement())
  user       String?
  status     String?
  progresso  Float?   @default(0)
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
  service    String?
}

model tft_player {
  id         Int       @id @unique(map: "id") @default(autoincrement()) @db.UnsignedInt
  account_id String    @db.VarChar(255)
  puuid      String    @db.VarChar(255)
  name       String?   @db.VarChar(255)
  created_at DateTime? @default(now()) @db.Timestamp(0)
  updated_at DateTime? @default(now()) @db.Timestamp(0)
}

model champion {
  id            Int       @id @unique(map: "id") @default(autoincrement()) @db.UnsignedInt
  name          String    @db.VarChar(255)
  api_name      String?   @db.VarChar(255)
  set           String?   @db.VarChar(255)
  trait_1       String?   @db.VarChar(255)
  trait_2       String?   @db.VarChar(255)
  trait_3       String?   @db.VarChar(255)
  cost          Int?
  avg_placement Float?    @db.Float
  winrate       Int?
  top4          Int?
  created_at    DateTime? @default(now()) @db.Timestamp(0)
  updated_at    DateTime? @default(now()) @db.Timestamp(0)
}

model champions_match_data {
  id           Int      @id @default(autoincrement())
  matchid      String?
  game_version String?
  placement    Int?
  champion     String?
  api_name     String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @default(now())
}
